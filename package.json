{
  "name": "cchub-backend-test",
  "version": "0.1.0",
  "description": "Technical assessment from cchub backend developer role",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/index.js",
    "build": "babel src --extensions .js,.ts --out-dir dist",
    "test": "cross-env NODE_ENV=test npm run db:drop && cross-env NODE_ENV=test npm run db:create && cross-env NODE_ENV=test npm run db:migrate && jest --config --detectOpenHandles  --runInBand -u --forceExit",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "CI=true cross-env NODE_ENV=test jest --forceExit --detectOpenHandles  --runInBand --coverage -u",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "coverage:report": "CI=true cross-env NODE_ENV=test nyc --reporter cobertura jest --forceExit --detectOpenHandles  --runInBand --coverage -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts|js|json)\"",
    "format:check": "prettier --ignore-path .gitignore --list-different \"**/*.+(ts|js|json)\"",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
    "db:create": "npm run sequelize db:create",
    "db:drop": "npm run sequelize db:drop",
    "db:migrate": "npm run sequelize db:migrate",
    "db:migrate:reset": "npm run sequelize db:migrate:undo:all",
    "db:seed": "npm run sequelize db:seed:all",
    "db:migrate:seed": "npm run sequelize db:migrate && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Muhire-Josue/cchub-backend-test.git"
  },
  "keywords": [
    "express",
    "typescript",
    "node",
    "postgres",
    "api"
  ],
  "author": "Muhire Josue",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.2.0",
    "@sentry/node": "^5.17.0",
    "@slack/web-api": "^6.7.2",
    "@types/minimist": "^1.2.1",
    "app-root-path": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^12.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.8.28",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "make-runnable": "^1.3.8",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "querystring": "^0.2.0",
    "request-ip": "^2.2.0",
    "sequelize": "^5.21.13",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^3.9.5",
    "uuid": "^8.3.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/app-root-path": "^1.2.4",
    "@types/async": "^3.2.4",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.32",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/event-emitter": "^0.3.3",
    "@types/express": "^4.17.6",
    "@types/express-useragent": "^1.0.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/request-ip": "^0.0.35",
    "@types/speakeasy": "^2.0.5",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.0.1",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-watch": "^7.0.0",
